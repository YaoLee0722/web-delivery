/**************************Step Two (group flag)*****************************/
/******************My account and Map******************/
.MyAccount{
	padding:15rem 10rem;
	width: 100%;
	background-color: #797a7c;
	background-image: url("background.jpg");
	background-size:cover;
}
.MyAccount-content{
	height: auto;
	width: 100%;
	display: flex;
	justify-content: center;
	border-radius: 10px;
}
.left{
	width: 100%;
	border-top-left-radius:10px;
	border-bottom-left-radius:10px;
	background-color: #01abaa;
}
.right{
	width: 100%;
	border-top-right-radius:10px;
	border-bottom-right-radius:10px;
	position:relative;
	background-color: #fea680;
}
.MyAccount-form{
	padding: 5rem;
	width: 100%;
}
.form{
	width: 100%;
	text-align: center;
	padding: 3rem 0;
}
.form-group{
	position: relative;
	padding: 2rem;
	width: 100%;
}
.MyAccount-heading{
	font-family: 'Playfair Display SC', serif;
	font-size: 6rem;
	color:  #01abaa;
	text-align: center;
	text-shadow: 0 0 0.1rem white;
	margin-bottom: 20%;
}

.ma-b:link,
.ma-b:visited{
	font-family: 'Balsamiq Sans', cursive , san-serif;
	text-decoration: none;
	display: inline-block;
	border: 1px solid #797a7c;
	background-color: black;
	color: white;
	outline: none;
	font-size:2.5rem;
	border-radius: 10px;
	margin: 2rem;
	padding: 1rem 2rem;
	transition: all .5 ease;
}
.ma-b:hover{
	background-color: #fedc3d;		
	border: 1px solid white;
	box-shadow: 0 0 9.2rem white;
}
.input{
	width:95%;
	outline:none;
	border:1px solid #b2d338;
	background-color: #fff;
	height: 7rem;
	font-size: 2rem;
	border-radius: 1000px;
	text-indent: 5px;
	box-shadow: 0 0 1rem rgba(0,0,0,.2);
	padding: 1rem;
	transition: all .6s ease;
}
.input:focus{
	width 100%;
}
.input:valid{
	border-left: 10px solid #1cf404;
	border-right: 10px solid #1cf404;
}
.input:invalid{
	border-left: 10px solid #ff0000;
	border-right: 10px solid #ff0000;
}
.form-group label{
	position: absolute;
	top : 52px;
	left: 58px;
	font-size: 1.6rem;
	background-color: white;
	visibility: hidden;
	transition: top .8s ease, left .8s ease;
}
.input:focus ~ label{
	top: 10px;
	left: 60px;
	padding: 0 1rem;
	box-shadow: 0 0 1.1rem rgba(0,0,0,.4);
	visibility: visible;
	border-radius: 10px;
}
.input:focus::placeholder{
	color: transparent;
}
.left{
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-evenly;
}
.mapheading{
	font-family: 'Playfair Display SC', serif;
	font-size: 6rem;
	color: #f8aa79;
	text-align:center;
	text-shadow: 0 0 .1rem white;
}
/**************Footer****************/
.footer{
	font-family: Garamond, serif;
	width: 100vw;
	display: flex;
	overflow: hidden;
	padding: 55px 0;
	box-sizing: border-box;
	background-color: black;
	color:white;
}
.footer > *{
	flex: 1 100%;
}
.footer-Cavallo{
	font-size: 1.4rem;
	margin-right:1.25em;
	margin-bottom: 2em;
	margin-left: 3em;
}
.footerlogo img{
	width: 55%;
	height: auto;
	padding-bottom: 1.5rem;
}
h2{
	font-weight:600;
	font-size: 2.6rem;
	padding-bottom: 1.75rem;
}
.footer ul{
	list-style: none;
	padding-left:0;
}
.footer ul img{
	height: 25px;
	width: 25px;
	padding-right: 1%;
	margin-top: -10px;
}
.footer li{
	font-size: 1.3rem;
	line-height: 2em;
	padding-left: 15px;
}
.footer a{
	text-decoration: none;
}
.footer-NewcastleUni{
	display: -webkit-flex;
	display: -moz-flex;
	display: -ms-flex;
	display: -o-flex;
	display: flex;
	flex-flow: row wrap;
}
.footer-NewcastleUni > *{
	flex: 1 50%;
	margin-right: 1.25em;
}
.box a{
	color: #b2d338;
}
.footer-Cavallo p{
	padding-right: 20%;
	color: white;
	padding-bottom: 3%;
}
.SocialMedia img{
	width:50px;
	height: 50px;
	display: inline-block;
	margin-right: 10px;
}
/******* small screen*********/
@media screen and (max-width: 940px){
.video{
	padding-top: 250px;
}
.header-box{
	padding-top: 250px;
}
.navbar{
	height:250px;
	padding: 0;
}
.navbar-list{
	display : block;
}
.navbar li:last-child{
	padding:2rem 0 0 1rem;
}
.heading-primary{
	font-size: 2rem;
}
.lead{
	font-size:1.2rem;
	padding-top:10px;
}
.navbar-item{
	padding:0.5rem 0 0.5rem 0;
}

.menu{
	padding: 0 0 0 0;
}
.menu-h{
	padding-top:1rem;
	padding-bottom:2rem;
	font-size: 8rem;
}
.menu-g{
	grid-template-columns: repeat(1 , 1fr);
}
.menu-g:last-child{
	padding-bottom:3rem;
}
.menu-container{
	width:80%;
	margin: 0 auto 0 auto;
}
.menuSaltedCaramel{
	font-size: 2rem;
}
.MyAccount{
	padding:0;
}
.MyAccount-content{
	display: inline-block;
	padding: 0;
}
.MyAccount-info{
	width:100vw;
}
.MyAccount-form{
	padding: 0;
}
.left{
	display:inline-block;
	width:127vw;
	margin-left: 0;
	padding: 5rem 0 5rem 0;
}
.iframe{
	width: 80vw;
	margin-left: 5rem;
}
}
/*other settings for small screens*/
@media screen and (min-width: 600px){
	.footer-NewcastleUni > *{
		flex: 1;
	}
	.footer-Cavallo{
		flex: 1 0px;
	}
	.footer-NewcastleUni{
		flex: 2 0px;
	}
}
@media (max-width: 600px){
	.footer{
		padding: 15px;
	}
	main{
		font-size: 55px;
	}
}
