@import url('https://fonts.googleapis.com/css2?family=Playfair+Display+SC:ital@1&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Balsamiq+Sans&display=swap');
*{
	padding: 0 0 0 0;
	margin: 0 0 0 0;
	box-sizing: border-box;
}
html{
	font-size: 62.5%;
}
body{
	background-color: rgb(27,27,27);
	overflow-x:hidden;
}
.header{
	height:90vh;
	position: relative;
}
.navbar{
	font-family: 'Balsamiq Sans', cursive , san-serif;
	height: 120px;
	width:100%;
	background-color:black;
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 0 40px;
	z-index:10;
	position: fixed;
}
.navbar-box{
	display: flex;
	align-items: center;
	font-size: 2rem;
	color:white;
}
.logo1 {
	padding-right: 5%;
}
.logo1 img{
	width: 190px;
	height: 95px
}
.heading-tertiary{
	font-size: 3.5rem;
}
.navbar-list{
	list-style: none;
	font-size: 2rem;
}
.navbar-item{
	display: inline-block;
}
.navbar-link:visited,
.navbar-link:link{
	text-decoration:none;
	color:white;
	padding: 3rem;
	transition: all .5s ease;
}

.navbar-link:hover{
	background-color:#fea680;
	color: white;
	border-radius: 70px;
}
.navbar-link-Order:visited,
.navbar-link-Order:link{
	text-decoration: none;
	color:white;
	padding:  2rem 3.5rem;
	border: 1px solid #fedc3d; /*should be same as 	"background-color:" in .navbar-link-Order:hover */
	border-radius: 200px;
	box-shadow: 0 0 3rem rgba(255,255,255, .5);
	transition: all .4s ease;
}
.navbar-link-Order:hover{
	background-color: #fedc3d;
	color: white;
}
.toggle{
	height: 5rem;
	width: 5rem;
	display:none;
}
.toggle div{
	height: 4px;
	width: 100%;
	background-color: white;
	margin: 1rem;
}
.header-container{
	width:100%;
	height:100%;
	background-size:cover;
	color:white;
}
.video{
	width:100%;
	height:100%;
	background-color:rgba(0,0,0,.4);
	position: absolute;
	bottom:0;
	right:0;
}
#video{
	width:100%;
	height:100%;
	object-fit:cover;
	position: relative;
	z-index: -1;
}
.header-box{
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	text-align:center;
	text-transform: uppercase;
	color: white;
}
.heading-primary{
	font-family: 'Playfair Display SC', serif;
	font-size: 6rem;
	font-weight:600;
	letter-spacing:3px;
}
#bold{
	color: #fedc3d;
	font-weight:800;
	font-size: 8rem;
}
.lead{
	font-size: 1.9rem;
	font-weight: 600;
	letter-spacing: 4px; 
}
.order-now:visited,
.order-now:link{
	text-decoration: none;
	display: inline-block;
	color:white;
	font-size: 2rem;
	padding: 2rem;
	margin-top: 4rem;
	border: 1px solid white;
	border-radius: 100px;
	transition: all .3s ease;
	box-shadow: 0 1rem 1rem rgba(240,107,55, .1);
	text-shadow: 0 1rem 1rem rgba(0,0,0, .6);
}
.order-now:hover{
	background-color: #fedc3d;		 /*should be same as 	"background-color:" in .navbar-link-Order:hover */
	border: 1px solid white;
	box-shadow: 0 0 15rem #fea680; 
}

/******************menu******************/
.menu{
	width:100%;
	padding: 10rem 5rem;
	background-image: linear-gradient(to left top, #fea680, #01abaa); 
}
.menu-h{
	font-family: 'Playfair Display SC', serif;
	font-size: 12rem;
	text-align: center;
	text-shadow:0 0 4rem #fedc3d;
	text-transform: uppercase;
	color: white;
	font-weight:800;
}
.menu-g{
	display: grid;
	grid-template-rows: repeat(2 , 1fr);
	grid-template-columns: repeat(4 , 1fr);
	justify-self:center;
	grid-gap: 2rem;
}
.menu-hold{
	font-family: Tahoma, sans-serif;
}
.menu-container{
	width:100%;
	height:50rem;
	background-color: white;
	border-radius: 5px;
	overflow: hidden;
	box-shadow: 0 0 2rem white;
}
.menu-container:hover{
	box-shadow: 0 0 2rem #000000;
	transform: scale(1.07);
}
.menu-p{
	font-size: 1.4rem;
	padding: 1rem;
	text-align: center;
}
